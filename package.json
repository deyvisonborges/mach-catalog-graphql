    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.3.4",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/throttler": "^5.1.2",
    "@prisma/adapter-pg": "^5.11.0",
    "@prisma/client": "^5.11.0",
